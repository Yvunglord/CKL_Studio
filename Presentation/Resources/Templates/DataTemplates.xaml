<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:CKL_Studio.Presentation.Views"
                    xmlns:vm="clr-namespace:CKL_Studio.Presentation.ViewModels">

    <DataTemplate DataType="{x:Type vm:EntryPointViewModel}">
        <views:EntryPointView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CklCreationViewModel}">
        <views:CKLCreationView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SourceInputViewModel}">
        <views:SourceInputView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RelationInputViewModel}">
        <views:RelationInputView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CklViewModel}">
        <views:CKLModelView/>
    </DataTemplate>

</ResourceDictionary>